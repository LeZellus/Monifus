<div class="p-4 grid gap-2">
    <h3 class="text-center dark:text-orange-500">{{ guild.name }}</h3>
    <div class="flex justify-between">
        <div class="flex items-center">
            <i class="w-4 mr-2 text-primary-500" data-feather="server"></i>
            <p class="font-bold">Serveur : </p>
        </div>
        <p class="text-tertiary-400 font-bold">{{ guild.server }}</p>
    </div>
    <div class="flex justify-between">
        <div class="flex justify-between">
            <i class="w-4 mr-2 text-primary-500" data-feather="users"></i>
            <p class="font-bold">Membres : </p>
        </div>
        <p class="text-green-400 font-bold">{{ guild.members|length }}</p>
    </div>
    <div class="flex justify-between">
        <div class="flex justify-between">
            <i class="w-4 mr-2 text-primary-500" data-feather="shield"></i>
            <p class="font-bold">Niveau : </p>
        </div>
        <p class="text-green-400 font-bold">{{ guild.level }}</p>
    </div>
    <div class="flex justify-between">
        <div class="flex justify-between">
            <i class="w-4 mr-2 text-primary-500" data-feather="user-plus"></i>
            <p class="font-bold">Level requis : </p>
        </div>
        <p class="text-green-400 font-bold">{{ guild.minimumLevel }}</p>
    </div>
    <div class="flex justify-between">
        <div class="flex justify-between">
            <i class="w-4 mr-2 text-primary-500" data-feather="award"></i>
            <p class="font-bold">SuccÃ¨s requis : </p>
        </div>
        <p class="text-green-400 font-bold">{{ guild.minimumSuccess }}</p>
    </div>
</div>